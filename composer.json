{
    "name": "felixnagel/mailfiles",
    "type": "typo3-cms-extension",
    "description": "Upload files and send download links via email. Uses plupload. Please see readme file.",
	"homepage": "https://github.com/mailfiles/pluploadfe",
	"support": {
		"issues": "https://github.com/fnagel/mailfiles/issues"
	},
	"authors": [
		{
			"name": "Felix Nagel",
			"email": "info@felixnagel.com",
			"homepage": "http://www.felixnagel.com",
			"role": "Developer"
		}
	],
    "require": {
        "php": ">=5.5.0,<7.3",
        "typo3/cms-core": ">=7.6.0,<=8.7",
        "felixnagel/pluploadfe": ">=1.4.0,<=2.2"
    },
    "autoload": {
        "psr-4": {
            "FelixNagel\\Mailfiles\\": "Classes"
        }
    },
	"replace": {
		"mailfiles": "self.version",
		"typo3-ter/mailfiles": "self.version"
	},
	"repositories": [
		{
			"type": "vcs",
			"url": "https://github.com/TYPO3/TYPO3.CMS"
		}
	]
}
